		</dependency>

		<dependency>
			<groupId>org.as3commons</groupId>
			<artifactId>as3commons-logging</artifactId>
			<version>2.7</version>
			<type>swc</type>
		</dependency>

		<dependency>
			<groupId>asmock</groupId>
			<artifactId>asmock</artifactId>
			<version>1.0</version>
			<type>swc</type>
			<scope>test</scope>
		</dependency>

		<dependency>
			<groupId>asmock</groupId>
			<artifactId>asmock-flexunit4.1</artifactId>
			<version>1.0</version>
			<type>swc</type>
			<scope>test</scope>
